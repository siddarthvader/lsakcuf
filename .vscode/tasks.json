{
	// See http://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "0.1.0",
	"tasks": [
		{
			"command": "tsc",
			"isShellCommand": true,
			"args": [
				"-p",
				"."
			],
			"showOutput": "silent",
			"problemMatcher": "$tsc"
		},
		{
			"command": "lessc",
			"isShellCommand": true,
			"args": [
				"styles.less",
				"styles.css"
			]
		}
	]
}